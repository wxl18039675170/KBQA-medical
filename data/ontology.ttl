@prefix ldt: <http://www.kg.org/> .
@prefix ns1: <ldt:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@base <http://www.kg.org/> .


rdf:type a rdf:Property ;
	rdfs:isDefinedBy <http://www.w3.org/1999/02/22-rdf-syntax-ns#> ;
	rdfs:label "type" ;
	rdfs:comment "The subject is an instance of a class." ;
	rdfs:range rdfs:Class ;
	rdfs:domain rdfs:Resource .


#################################################################
#    Object Properties
#################################################################

###  http://www.kg.org/acompany_with
ns1:acompany_with rdf:type owl:ObjectProperty ;
               rdfs:domain ns1:Disease ;
               rdfs:range ns1:Disease .


###  http://www.kg.org/belongs_to
ns1:belongs_to rdf:type owl:ObjectProperty ;
            rdfs:domain ns1:Department ;
            rdfs:range ns1:Department .


###  http://www.kg.org/common_drug
ns1:common_drug rdf:type owl:ObjectProperty ;
             rdfs:domain ns1:Disease ;
             rdfs:range ns1:Drug .


###  http://www.kg.org/cure_department
ns1:cure_department rdf:type owl:ObjectProperty ;
                 rdfs:domain ns1:Disease ;
                 rdfs:range ns1:Department .


###  http://www.kg.org/do_eat
ns1:do_eat rdf:type owl:ObjectProperty ;
        rdfs:domain ns1:Disease ;
        rdfs:range ns1:Food .


###  http://www.kg.org/drugs_of
ns1:drugs_of rdf:type owl:ObjectProperty ;
          rdfs:domain ns1:Producer ;
          rdfs:range ns1:Drug .


###  http://www.kg.org/has_symptom
ns1:has_symptom rdf:type owl:ObjectProperty ;
             rdfs:domain ns1:Disease ;
             rdfs:range ns1:Symptom .


###  http://www.kg.org/need_check
ns1:need_check rdf:type owl:ObjectProperty ;
            rdfs:domain ns1:Disease ;
            rdfs:range ns1:Check .


###  http://www.kg.org/no_eat
ns1:no_eat rdf:type owl:ObjectProperty ;
        rdfs:domain ns1:Disease ;
        rdfs:range ns1:Food .


###  http://www.kg.org/recommand_drug
ns1:recommand_drug rdf:type owl:ObjectProperty ;
                rdfs:domain ns1:Disease ;
                rdfs:range ns1:Drug .


###  http://www.kg.org/recommand_eat
ns1:recommand_eat rdf:type owl:ObjectProperty ;
               rdfs:domain ns1:Disease ;
               rdfs:range ns1:Food .


